{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": [
        "yii2",
        "framework",
        "basic",
        "project template"
    ],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "dmstr/yii2-adminlte-asset": "*",
        "schmunk42/yii2-giiant": "@stable",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "2amigos/yii2-selectize-widget": "@dev",
        "kartik-v/yii2-widget-spinner": "@dev",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-widget-depdrop": "^1.0",
        "drsdre/yii2-wizardwidget": "*",
        "himiklab/yii2-recaptcha-widget": "*",
        "wbraganca/yii2-dynamicform": "@dev",
        "fxp/composer-asset-plugin": "1.*",
        "richardfan1126/yii2-js-register": "^0.0.1",
        "phpoffice/phpspreadsheet": "^1.8",
        "ramsey/uuid": "^3.9",
        "vova07/yii2-imperavi-widget": "*",
        "richardfan1126/yii2-sortable-gridview": "*",
        "kartik-v/yii2-date-range": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "insolita/yii2-migration-generator": "*",
        "codeception/codeception": "^4.1",
        "codeception/specify": "^1.0",
        "codeception/verify": "^1.3",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-rest": "^1.4",
        "codeception/module-yii2": "^1.1",
        "codeception/module-filesystem": "^1.0"
    },
    "config": {
        "platform": {
            "php": "7.0"
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        },
        "allow-plugins": {
            "composer/installers": true,
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    }
}
